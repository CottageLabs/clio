<%
if f.object.model_class == ::Dataset
  options = DatasetResourceTypesService.select_all_options
else
  options = ArticleResourceTypesService.select_all_options
end
%>
<div class="resource-type-editor">
  <%= f.input :resource_type,
    as: :multi_value_select,
    collection: options,
    include_blank: true,
    prompt: 'Select resource type',
    input_html: { class: 'form-control' }
  %>
</div>
