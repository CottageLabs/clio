<div class="multi-nested">
  <%= f.input :identifier_nested,
    as: :object_identifier,
    input_html: {
      class: '',
      data: {name: :identifier_nested},
    },
    required: f.object.required?(:identifier_nested)
  %>
  <button type="button" class="btn btn-link add">
    <span class="glyphicon glyphicon-plus"></span>
    <span class="controls-add-text">Add another Identifier</span>
  </button>
</div>
